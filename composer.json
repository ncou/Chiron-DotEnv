{
    "name": "chiron/dotenv",
    "type": "library",
    "description": "Registers environment variables from a .env file",
    "keywords": ["environment", "env", "dotenv"],
    "homepage": "https://github.com/ncou/Chiron-DotEnv",
    "license" : "MIT",
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "phpstan/phpstan": "^0.9.2"
    },
    "autoload": {
        "psr-4": { "Chiron\\Dotenv\\": "" },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "minimum-stability": "dev",
    "scripts": {
        "phpunit": "phpunit --colors=always",
        "phpstan": "phpstan analyse src --level=0"
    }
}